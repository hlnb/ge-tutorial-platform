# Copy this file to .env and fill in your actual values

# EmailJS Configuration
VITE_EMAILJS_PUBLIC_KEY=your_emailjs_public_key_here
VITE_EMAILJS_SERVICE_ID=your_emailjs_service_id_here
VITE_EMAILJS_TEMPLATE_ID=your_emailjs_template_id_here

# reCAPTCHA Configuration
VITE_RECAPTCHA_SITE_KEY=your_recaptcha_site_key_here
VITE_RECAPTCHA_SECRET_KEY=your_recaptcha_secret_key_here

# Brevo Email Service Configuration
BREVO_API_KEY=your_brevo_api_key_here
BREVO_TUTORIAL_LIST_ID=your_tutorial_list_id_here
BREVO_BLOG_LIST_ID=your_blog_list_id_here

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/graphitedge
NODE_ENV=development

# GitHub OAuth for Sveltia CMS
OAUTH_GITHUB_CLIENT_ID=your_github_oauth_client_id_here
OAUTH_GITHUB_CLIENT_SECRET=your_github_oauth_client_secret_here

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id_here
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id_here
VITE_FIREBASE_APP_ID=your_firebase_app_id_here
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id_here